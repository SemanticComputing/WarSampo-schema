##  CIDOC CRM

crm:E39_Actor a rdfs:Class ;
    skos:prefLabel "Actor"@en ;
    skos:prefLabel "Toimija"@fi .

crm:E21_Person a rdfs:Class ;
    skos:prefLabel "Person"@en, "Henkilö"@fi ;
    rdfs:subClassOf crm:E39_Actor .
    
crm:E74_Group a rdfs:Class ;
    skos:prefLabel "Group"@en, "Ryhmä"@fi ;
    rdfs:subClassOf crm:E39_Actor .

crm:E5_Event a rdfs:Class ;
    skos:prefLabel "Event"@en ;
    skos:prefLabel "Tapahtuma"@fi .

crm:E7_Activity a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Activity"@en ;
    skos:prefLabel "Toiminta"@fi .

crm:E9_Move a rdfs:Class ;
    rdfs:subClassOf crm:E7_Activity ;
    skos:prefLabel "Move"@en ;
    skos:prefLabel "Liike"@fi .

crm:E69_Death a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Death"@en ;
    skos:prefLabel "Kuolema"@fi .

crm:E67_Birth a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Birth"@en ;
    skos:prefLabel "Syntymä"@fi .

crm:E66_Formation a rdfs:Class ;
    rdfs:subClassOf crm:E7_Activity ;
    skos:prefLabel "Formation of a Military Group"@en ;
        skos:prefLabel "Joukko-osaston muodostaminen"@fi .

crm:E55_Type a rdfs:Class ;
    skos:prefLabel "Type"@en,
        "Tyyppi"@fi .

crm:E31_Document a rdfs:Class ;
    skos:prefLabel "Document"@en,
        "Dokumentti"@fi .

crm:E24_Physical_Man-Made_Thing a rdfs:Class ;
    skos:prefLabel "Esine"@fi .

### Properties

crm:P100_was_death_of a rdf:Property ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:domain crm:E69_Death ;
    skos:prefLabel "kuollut"@fi ;
    rdfs:range crm:E21_Person .

crm:P98_brought_into_life a rdf:Property ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:domain crm:E67_Birth ;
    skos:prefLabel "syntynyt"@fi ;
    rdfs:range crm:E21_Person .

crm:P143_joined a rdf:Property ;
    rdfs:range crm:E39_Actor ;
    skos:prefLabel "liittyjä"@fi ;
    rdfs:subPropertyOf crm:P11_had_participant .

crm:P144_joined_with a rdf:Property ;
    skos:prefLabel "liittynyt osastoon"@fi ;
    rdfs:range crm:E74_Group .

crm:P95_has_formed a rdf:Property ;
    skos:prefLabel "muodostettu"@fi ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:range crm:E74_Group .

crm:P3_has_note skos:prefLabel "huomautus"@fi .
crm:P4_has_time-span skos:prefLabel "tapahtuma-aika"@fi .
crm:P7_took_place_at skos:prefLabel "tapahtumapaikka"@fi .
crm:P70_documents skos:prefLabel "dokumentoi"@fi .
crm:P70i_is_documented_in skos:prefLabel "dokumentoitu"@fi .

crm:P96_by_mother skos:prefLabel "äiti"@fi .
crm:P97_from_father skos:prefLabel "isä"@fi .

crm:P107_1_kind_of_member skos:prefLabel "henkilön rooli"@fi .
crm:P141_assigned skos:prefLabel "myönnettiin"@fi .

dct:description skos:prefLabel "kuvaus"@fi .
dct:source skos:prefLabel "lähde"@fi .


##  PHOTOGRAPHS

# The photo schema will be converted to CIDOC CRM compliant scheme soon

:Photograph a rdfs:Class ;
    rdfs:subClassOf dctype:Image , crm:E31_Document ;
    skos:prefLabel "Photograph"@en ;
    skos:prefLabel "Valokuva"@fi .

:place_string a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:string ;
    skos:prefLabel "Place"@en ,
        "Paikka"@fi ;
   dct:description "Original place string in photo metadata"@en ,
         "Alkuperäinen paikkakuvaus metadatassa"@fi .

:photographer_string a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:string ;
    skos:prefLabel "Place"@en ,
        "Paikka"@fi ;
   dct:description "Original photographer string in photo metadata"@en ,
         "Alkuperäinen valokuvaaja metadatassa"@fi .

:unit a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range :MilitaryUnit ;
    skos:prefLabel "Military unit"@en ,
        "Joukko-osasto"@fi .

:theme a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:string ;
    skos:prefLabel "Subject code"@en,
         "Kuva-aihekoodi"@fi ;
   dct:description "Subject code in the original metadata"@en ,
         "Kuva-aihekoodi alkuperäisestä metadatasta"@fi .


##  ACTORS

:MilitaryPerson a rdfs:Class ;
    skos:prefLabel "Military Person"@en, "Sotilashenkilö"@fi ;
    rdfs:subClassOf crm:E21_Person .

:PoliticalPerson a rdfs:Class ;
    skos:prefLabel "Political Person"@en, "Poliittinen henkilö"@fi ;
    rdfs:subClassOf crm:E21_Person .

:MilitaryUnit a rdfs:Class ;
    skos:prefLabel "Sotilasyksikkö"@fi, "Military Unit"@en ;
    rdfs:subClassOf crm:E74_Group .


##  EVENTS

:Event a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Event"@en ;
    skos:prefLabel "Tapahtuma"@fi .

:Activity a rdfs:Class ;
    rdfs:subClassOf :Event, crm:E7_Activity ;
    skos:prefLabel "Activity"@en ;
    skos:prefLabel "Toiminta"@fi .

:MilitaryActivity a rdfs:Class ;
    skos:prefLabel "Military Activity"@en ;
    skos:prefLabel "Sotatoimi"@fi ;
    rdfs:subClassOf :Activity.

:PoliticalActivity a rdfs:Class ;
    skos:prefLabel "Political Activity"@en ;
    skos:prefLabel "Poliittinen toiminta"@fi ;
    rdfs:subClassOf :Activity .

:Battle a rdfs:Class ;
    skos:prefLabel "Battle"@en ;
    skos:prefLabel "Taistelu"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:Bombardment a rdfs:Class ;
    skos:prefLabel "Bombardment"@en ;
    skos:prefLabel "Pommitus"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:Promotion a rdfs:Class ;
    skos:prefLabel "Promotion"@en ;
    skos:prefLabel "Ylennys"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:Death a rdfs:Class ;
    rdfs:subClassOf :Event, crm:E69_Death ;
    skos:prefLabel "Death"@en ;
    skos:prefLabel "Kuolema"@fi .

:Birth a rdfs:Class ;
    rdfs:subClassOf :Event, crm:E67_Birth ;
    skos:prefLabel "Birth"@en ;
    skos:prefLabel "Syntymä"@fi .

:Wounding a rdfs:Class ;
    rdfs:subClassOf :Event ;
    skos:prefLabel "Wounding"@en ;
        skos:prefLabel "Haavoittuminen"@fi .

:Disappearing a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Disappearing"@en ;
        skos:prefLabel "Katoaminen"@fi .

:UnitJoining a rdfs:Class ;
    skos:prefLabel "Unit Joining"@en ;
    skos:prefLabel "Joukko-osaston liittyminen suurempaan joukko-osastoon"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:MedalAwarding a rdfs:Class ;
      rdfs:subClassOf crm:E13_Attribute_Assignment ;
    skos:prefLabel "Kunniamitalin myöntäminen"@fi, "Medal Awarding"@en .

:PersonJoining a rdfs:Class ;
    skos:prefLabel "Person joining a Military Unit"@en ;
    skos:prefLabel "Henkilön liittyminen joukko-osastoon"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:UnitNaming a rdfs:Class ;
    skos:prefLabel "Unit Renaming"@en ;
    skos:prefLabel "Joukko-osaston nimeäminen"@fi ;
    rdfs:subClassOf crm:E66_Formation, :MilitaryActivity .

:TroopMovement a rdfs:Class ;
    skos:prefLabel "Troop Movement"@en ;
    skos:prefLabel "Joukkojen liikutus"@fi ;
    rdfs:subClassOf :MilitaryActivity,
        crm:E9_Move .


##  RANKS

:Rank a rdfs:Class ;
    rdfs:subClassOf crm:E55_Type ;
    skos:prefLabel "Military Rank"@en,
        "Sotilasarvo"@fi .

:hasRank a rdf:Property ;
    rdfs:domain atypes:MilitaryPerson,
        etypes:Promotion ;
    rdfs:range ranks:Rank ;
    skos:prefLabel "Military Rank"@en,
        "Sotilasarvo"@fi .

org:rankSeniorTo skos:prefLabel "ylempi kuin"@fi .
org:equalTo skos:prefLabel "vastaa"@fi .


##  WAR DIARIES

:WarDiary a rdfs:Class ;
    rdfs:subClassOf crm:E31_Document ;
    skos:prefLabel "War Diary"@en,
        "Sotapäiväkirja"@fi .


##  MEDALS

:Medal a rdfs:Class ;
    rdfs:subClassOf crm:E24_Physical_Man-Made_Thing ;
    skos:prefLabel "Kunniamitali"@fi .


##  SOURCES

:Source a rdfs:Class ;
    skos:prefLabel "Lähde"@fi ;
     rdfs:subClassOf crm:E31_Document ;
    skos:prefLabel "Source"@fi .


## PLACES

:Place a rdfs:Class;
skos:prefLabel "Historiallinen paikka"@fi, "Historical place"@en ;
       rdfs:subClassOf crm:E53_Place . 

:Karjalan_paikka a rdfs:Class ; 
    skos:prefLabel  "Karjalan paikka"@fi, "Karelian place"@en ;
    rdfs:subClassOf    :Place .

suo:valtio skos:prefLabel "Valtio"@fi, "Country"@en  ;
    rdfs:subClassOf    :Place .

suo:laani skos:prefLabel "lääni"@fi, "county"@en ;
    rdfs:subClassOf    :Place .

suo:kunta skos:prefLabel "kunta"@fi, "municipality"@en ;
    rdfs:subClassOf    :Place .

:Kyla a rdfs:Class ;
       owl:sameAs <http://www.yso.fi/onto/suo/kyla> ;
       skos:prefLabel "kylä"@fi, "village"@en ;
rdfs:subClassOf    :Karjalan_paikka .  
        
:Kirkonkyla_kaupunki
      a rdfs:Class ;
      skos:prefLabel  "kirkonkylä, kaupunki"@fi, "town"@en ;
    rdfs:subClassOf    :Karjalan_paikka .
            
:Rakennettu_kohde
       a rdfs:Class ;
       owl:sameAs <http://www.yso.fi/onto/suo/rakennettu_kohde> ;
      skos:prefLabel "rakennettu kohde"@fi, "man-made feature"@en ;
    rdfs:subClassOf    :Karjalan_paikka .        

:Symboli a rdfs:Class ;
       skos:prefLabel  "symboli"@fi, "symbol"@en ;
rdfs:subClassOf :Karjalan_paikka .

:Maastokohde
      a rdfs:Class ;
       owl:sameAs <http://www.yso.fi/onto/gns/hypsographic_feature> ;
      skos:prefLabel  "maastokohde"@fi, "hypsographic feature"@en  ;
    rdfs:subClassOf    :Karjalan_paikka .

placeTypes:Vesimuodostuma
       a rdfs:Class ;
      owl:sameAs <http://www.yso.fi/onto/suo/vesimuodostuma> ;
       skos:prefLabel "vesimuodostuma"@fi, "body of water"@en ;        
    rdfs:subClassOf    :Karjalan_paikka .


