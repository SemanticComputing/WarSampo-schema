## PHOTOGRAPHS

:Photograph a rdfs:Class , owl:Class ;
    rdfs:subClassOf dctype:Image , crm:E31_Document , crm:E38_Image ;
    skos:prefLabel "Photograph"@en, "Valokuva"@fi .

wphs:Size a rdfs:Class , owl:Class ;
    skos:prefLabel "Image size"@en, "Kuvakoko"@fi .

wphs:lg a wphs:Size ;
    skos:prefLabel "Full size"@en , "Suuri"@fi .

wphs:md a wphs:Size ;
    skos:prefLabel "Medium size"@en , "Keskikokoinen"@fi .

wphs:sm a wphs:Size ;
    skos:prefLabel "Thumbnail"@en , "Pieni"@fi .

wphs:size a owl:ObjectProperty ;
    rdfs:domain crm:E73_Information_Object ;
    rdfs:range wphs:Size ;
    skos:prefLabel "Image size"@en ,
        "Kuvakoko"@fi ;
    dct:description "Indicates the size of the image (thumbnail, medium, full size)."@en ,
        "Ilmaisee kuvatiedoston kuvan koon (esikatselukuva, keskikoko, suuri koko)"@fi .

wphs:place_string a owl:DatatypeProperty ;
    rdfs:domain :Photograph ;
    rdfs:range rdfs:Literal ;
    skos:prefLabel "Place"@en, "Paikka"@fi ;
    dct:description "Original place string in photo metadata"@en ,
         "Alkuperäinen paikkakuvaus metadatassa"@fi .

wphs:photographer_string a owl:DatatypeProperty ;
    rdfs:domain :Photograph ;
    rdfs:range rdfs:Literal ;
    skos:prefLabel "Photographer"@en , "Valokuvaaja"@fi ;
    dct:description "Original photographer string in photo metadata"@en ,
        "Alkuperäinen valokuvaaja metadatassa"@fi .

wphs:theme a owl:DatatypeProperty ;
    rdfs:domain :Photograph ;
    rdfs:range rdfs:Literal ;
    skos:prefLabel "Subject code"@en ,
        "Kuva-aihekoodi"@fi ;
    dct:description "Subject code in the original metadata"@en ,
        "Kuva-aihekoodi alkuperäisestä metadatasta"@fi .

wphs:is_color a owl:DatatypeProperty ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:boolean ;
    skos:prefLabel "Color photograph"@en ,
        "Värikuva"@fi ;
    dct:description "Indicates whether or not the photograph has color"@en ,
        "Ilmaisee onko valokuva värillinen"@fi .

