wacs:hasConflict a rdf:Property ;
    rdfs:domain :MilitaryUnit ;
    rdfs:range wcf:Conflict ;
    skos:prefLabel "participated in conflict"@en ,
        "osallistui sotaan"@fi .

## SOURCES

:Source a rdfs:Class ;
    rdfs:subClassOf crm:E31_Document ;
    skos:prefLabel "Lähde"@fi ;
    skos:prefLabel "Source"@fi .

## PLACES

hipla:Place a rdfs:Class;
    skos:prefLabel "Historiallinen paikka"@fi , "Historical place"@en ;
    rdfs:subClassOf crm:E53_Place .

gs:sfWithin a owl:ObjectProperty ;
    rdfs:label "Preferred part of relation"@en ;
    rdfs:domain crm:E53_Place ;
    rdfs:range crm:E53_Place .

hipla:topology a owl:ObjectProperty ;
    rdfs:label "Temporal part of relations"@en ;
    rdfs:domain crm:E53_Place ;
    rdfs:range crm:E53_Place .

    ## ADMINISTRATIVE

suo:valtio skos:prefLabel "Valtio"@fi , "Country"@en ;
