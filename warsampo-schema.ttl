## CIDOC CRM

crm:E4_Period a rdfs:Class ;
    skos:prefLabel "Ajanjakso"@fi, "Period"@en .

crm:E39_Actor a rdfs:Class ;
    skos:prefLabel "Actor"@en ;
    skos:prefLabel "Toimija"@fi .

crm:E21_Person a rdfs:Class ;
    skos:prefLabel "Person"@en , "Henkilö"@fi ;
    rdfs:subClassOf crm:E39_Actor .

crm:E74_Group a rdfs:Class ;
    skos:prefLabel "Group"@en , "Ryhmä"@fi ;
    rdfs:subClassOf crm:E39_Actor .

crm:E5_Event a rdfs:Class ;
    skos:prefLabel "Event"@en ;
    skos:prefLabel "Tapahtuma"@fi .

crm:E7_Activity a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Activity"@en ;
    skos:prefLabel "Toiminta"@fi .

crm:E9_Move a rdfs:Class ;
    rdfs:subClassOf crm:E7_Activity ;
    skos:prefLabel "Move"@en ;
    skos:prefLabel "Liike"@fi .

crm:E63_Beginning_of_Existence a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Beginning of Existence"@en ;
    skos:prefLabel "Olemassaolon alkaminen"@fi .

crm:E65_Creation a rdfs:Class ;
    rdfs:subClassOf crm:E7_Activity, crm:E63_Beginning_of_Existence ;
    skos:prefLabel "Creation"@en ;
    skos:prefLabel "Luominen"@fi .

crm:E69_Death a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Death"@en ;
    skos:prefLabel "Kuolema"@fi .

crm:E67_Birth a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Birth"@en ;
    skos:prefLabel "Syntymä"@fi .

crm:E68_Dissolution a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Dissolution"@en ;
    skos:prefLabel "Lakkauttaminen"@fi .

crm:E66_Formation a rdfs:Class ;
    rdfs:subClassOf crm:E7_Activity ;
    skos:prefLabel "Formation"@en ;
    skos:prefLabel "Muodostaminen"@fi .

crm:E55_Type a rdfs:Class ;
    skos:prefLabel "Type"@en ,
        "Tyyppi"@fi .

crm:E31_Document a rdfs:Class ;
    skos:prefLabel "Document"@en ,
        "Dokumentti"@fi .

crm:E24_Physical_Man-Made_Thing a rdfs:Class ;
    skos:prefLabel "Esine"@fi .

crm:E53_Place a rdfs:Class ;
    skos:prefLabel "Place"@en ;
    skos:prefLabel "Paikka"@fi .

### Properties

crm:P100_was_death_of a rdf:Property ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:domain crm:E69_Death ;
    skos:prefLabel "kuollut"@fi ;
    rdfs:range crm:E21_Person .

crm:P98_brought_into_life a rdf:Property ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:domain crm:E67_Birth ;
    skos:prefLabel "syntynyt"@fi ;
    rdfs:range crm:E21_Person .

crm:P99_dissolved a rdf:Property ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:domain :Dissolution ;
    skos:prefLabel "lakkautettu"@fi ;
    rdfs:range crm:E74_Group .

crm:P143_joined a rdf:Property ;
    rdfs:range crm:E39_Actor ;
    skos:prefLabel "liittyjä"@fi ;
    rdfs:subPropertyOf crm:P11_had_participant .

crm:P144_joined_with a rdf:Property ;
    skos:prefLabel "liittynyt osastoon"@fi ;
    rdfs:range crm:E74_Group .

crm:P95_has_formed a rdf:Property ;
    skos:prefLabel "muodostettu"@fi ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    rdfs:range crm:E74_Group .

crm:P67_refers_to a rdf:Property ;
    skos:prefLabel "viittaa"@fi ;
    rdfs:range crm:E1_CRM_Entity ;
    rdfs:domain crm:E89_Propositional_Object .

crm:P3_has_note skos:prefLabel "huomautus"@fi .
crm:P4_has_time-span skos:prefLabel "tapahtuma-aika"@fi .
crm:P7_took_place_at skos:prefLabel "tapahtumapaikka"@fi .
crm:P70_documents skos:prefLabel "dokumentoi"@fi .
crm:P70i_is_documented_in skos:prefLabel "dokumentoitu"@fi .

crm:P96_by_mother skos:prefLabel "äiti"@fi .
crm:P97_from_father skos:prefLabel "isä"@fi .

crm:P107_1_kind_of_member skos:prefLabel "henkilön rooli"@fi .
crm:P141_assigned skos:prefLabel "myönnettiin"@fi .

dct:description skos:prefLabel "kuvaus"@fi .
dct:source skos:prefLabel "lähde"@fi .


## PHOTOGRAPHS

:Photograph a rdfs:Class ;
    rdfs:subClassOf dctype:Image , crm:E31_Document , crm:E38_Image ;
    skos:prefLabel "Photograph"@en ;
    skos:prefLabel "Valokuva"@fi .

wphs:Size a rdfs:Class ;
    skos:prefLabel "Image size"@en ;
    skos:prefLabel "Kuvakoko"@fi .

wphs:lg a wphs:Size ;
    skos:prefLabel "Full size"@en , "Suuri"@fi .

wphs:md a wphs:Size ;
    skos:prefLabel "Medium size"@en , "Keskikokoinen"@fi .

wphs:sm a wphs:Size ;
    skos:prefLabel "Thumbnail"@en , "Pieni"@fi .

wphs:size a rdf:Property ;
    rdfs:domain crm:E73_Information_Object ;
    rdfs:range wphs:Size ;
    skos:prefLabel "Image size"@en ,
        "Kuvakoko"@fi ;
    dct:description "Indicates the size of the image (thumbnail, medium, full size)."@en ,
        "Ilmaisee kuvatiedoston kuvan koon (esikatselukuva, keskikoko, suuri koko)"@fi .

wphs:place_string a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:string ;
    skos:prefLabel "Place"@en ,
        "Paikka"@fi ;
    dct:description "Original place string in photo metadata"@en ,
         "Alkuperäinen paikkakuvaus metadatassa"@fi .

wphs:photographer_string a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:string ;
    skos:prefLabel "Photographer"@en ,
        "Valokuvaaja"@fi ;
    dct:description "Original photographer string in photo metadata"@en ,
        "Alkuperäinen valokuvaaja metadatassa"@fi .

wphs:theme a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:string ;
    skos:prefLabel "Subject code"@en ,
        "Kuva-aihekoodi"@fi ;
    dct:description "Subject code in the original metadata"@en ,
        "Kuva-aihekoodi alkuperäisestä metadatasta"@fi .

wphs:is_color a rdf:Property ;
    rdfs:domain :Photograph ;
    rdfs:range xsd:boolean ;
    skos:prefLabel "Color photograph"@en ,
        "Värikuva"@fi ;
    dct:description "Indicates whether or not the photograph has color"@en ,
        "Ilmaisee onko valokuva värillinen"@fi .

# Other properties used:
# :Photograph crm:P138i_has_representation -> (crm:E73_Information_Object) resource describing the image file of the photo
# crm:E73_Information_Object sch:contentUrl -> (uri) photo URL
# :Photograph dct:source -> (:Source) photo source
# :Photograph skos:prefLabel -> (string) photo label (same as description)
# :Photograph dct:description -> (string) photo description


## ACTORS

:Person a rdfs:Class ;
    skos:prefLabel "Person"@en , "Henkilö"@fi ;
    rdfs:subClassOf crm:E21_Person .

:Group a rdfs:Class ;
    skos:prefLabel "Ryhmä"@fi , "Group"@en ;
    rdfs:subClassOf crm:E74_Group .

:MilitaryPerson a rdfs:Class ;
    skos:prefLabel "Military Person"@en , "Sotilashenkilö"@fi ;
    rdfs:subClassOf :Person .

:PoliticalPerson a rdfs:Class ;
    skos:prefLabel "Political Person"@en , "Poliittinen henkilö"@fi ;
    rdfs:subClassOf :Person .

:MilitaryUnit a rdfs:Class ;
    skos:prefLabel "Sotilasyksikkö"@fi , "Military Unit"@en ;
    rdfs:subClassOf :Group .

wacs:covernumber a rdf:Property ;
    rdfs:domain :Group ;
    rdfs:range xsd:string ;
    skos:prefLabel "Cover Number "@en ,
        "Peiteluku"@fi .

wacs:knight a rdf:Property ;
    rdfs:domain :Person ;
    rdfs:range xsd:integer ;
    skos:prefLabel "MRR numero"@fi, "Number of Knight of Mannerheim Cross"@en .

## EVENTS

:Event a rdfs:Class ;
    rdfs:subClassOf crm:E5_Event ;
    skos:prefLabel "Event"@en ;
    skos:prefLabel "Tapahtuma"@fi .

:Activity a rdfs:Class ;
    rdfs:subClassOf :Event , crm:E7_Activity ;
    skos:prefLabel "Activity"@en ;
    skos:prefLabel "Toiminta"@fi .

:MilitaryActivity a rdfs:Class ;
    skos:prefLabel "Military Activity"@en ;
    skos:prefLabel "Sotatoimi"@fi ;
    rdfs:subClassOf :Activity.

:PoliticalActivity a rdfs:Class ;
    skos:prefLabel "Political Activity"@en ;
    skos:prefLabel "Poliittinen toiminta"@fi ;
    rdfs:subClassOf :Activity .

:Battle a rdfs:Class ;
    skos:prefLabel "Battle"@en ;
    skos:prefLabel "Taistelu"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:Bombardment a rdfs:Class ;
    skos:prefLabel "Bombardment"@en ;
    skos:prefLabel "Pommitus"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:Promotion a rdfs:Class ;
    skos:prefLabel "Promotion"@en ;
    skos:prefLabel "Ylennys"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:Death a rdfs:Class ;
    rdfs:subClassOf :Event , crm:E69_Death ;
    skos:prefLabel "Death"@en ;
    skos:prefLabel "Kuolema"@fi .

:Birth a rdfs:Class ;
    rdfs:subClassOf :Event , crm:E67_Birth ;
    skos:prefLabel "Birth"@en ;
    skos:prefLabel "Syntymä"@fi .

:Wounding a rdfs:Class ;
    rdfs:subClassOf :Event ;
    skos:prefLabel "Wounding"@en ;
    skos:prefLabel "Haavoittuminen"@fi .

:Disappearing a rdfs:Class ;
    rdfs:subClassOf :Event ;
    skos:prefLabel "Disappearing"@en ;
        skos:prefLabel "Katoaminen"@fi .

:Formation a rdfs:Class ;
    rdfs:subClassOf :MilitaryActivity , crm:E66_Formation ;
    skos:prefLabel "Formation of a Military Group"@en ;
    skos:prefLabel "Joukko-osaston muodostaminen"@fi .

:Dissolution a rdfs:Class ;
    rdfs:subClassOf :MilitaryActivity , crm:E68_Dissolution ;
    skos:prefLabel "Dissolution of a Military Group"@en ;
    skos:prefLabel "Joukko-osaston lakkauttaminen"@fi .

:UnitJoining a rdfs:Class ;
    skos:prefLabel "Unit Joining"@en ;
    skos:prefLabel "Joukko-osaston liittyminen suurempaan joukko-osastoon"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:MedalAwarding a rdfs:Class ;
    rdfs:subClassOf :Activity ;
    skos:prefLabel "Kunniamitalin myöntäminen"@fi, "Medal Awarding"@en .

:PersonJoining a rdfs:Class ;
    skos:prefLabel "Person joining a Military Unit"@en ;
    skos:prefLabel "Henkilön liittyminen joukko-osastoon"@fi ;
    rdfs:subClassOf :MilitaryActivity .

:UnitNaming a rdfs:Class ;
    skos:prefLabel "Unit Naming"@en ;
    skos:prefLabel "Joukko-osaston nimeäminen"@fi ;
    rdfs:subClassOf crm:E66_Formation , :MilitaryActivity .

:TroopMovement a rdfs:Class ;
    skos:prefLabel "Troop Movement"@en ;
    skos:prefLabel "Joukkojen liikutus"@fi ;
    rdfs:subClassOf :MilitaryActivity ,
        crm:E9_Move .

:Photography a rdfs:Class ;
    rdfs:subClassOf :Activity, crm:E65_Creation ;
    skos:prefLabel "Photography"@en ;
    skos:prefLabel "Valokuvaus"@fi .

wevs:related_period a rdf:Property ;
   rdfs:domain :Event ;
   rdfs:range crm:E4_Period ;
   skos:prefLabel "Related period"@en ,
        "Aikakausi"@fi .

# Properties used:
# (any) crm:P4_has_time-span -> (crm:E52_Time-Span)
# (any) crm:P7_took_place_at -> (:Place / PNR place) place
# (any) crm:P14_carried_out_by -> (crm:E39_Actor) active actor: in photography the photographer
# (any) crm:P11_had_participant -> (crm:E39_Actor) any actor involved
# (any) dct:source -> (:Source) source
# (any) skos:prefLabel -> (string) label (same as description)
# (any) dct:description -> (string) description
#
# :Photography crm:P94_has_created -> (:Photograph)
#
# :Death crm:P100_was_death_of -> (:Person)
#
# :Birth crm:P98_brought_into_life -> (:Person)
#
# :MedalAwarding crm:P141_assigned -> (:Medal)
# :MedalAwarding crm:P11_had_participant -> (:Person) the person getting the medal
#
# crm:E66_Formation P95_has_formed -> (:MilitaryUnit) formed unit
#
# :UnitNaming P95_has_formed -> (:MilitaryUnit) (re)named unit
#
# :TroopMovement P95_has_formed -> (:MilitaryUnit) concentrated unit
#
# :UnitJoining P143_joined -> (:MilitaryUnit) the unit joining a larger unit
# :UnitJoining P144_joined_with -> (:MilitaryUnit) the larger unit with which the
#                                                  smaller unit is joining


## RANKS

:Rank a rdfs:Class ;
    rdfs:subClassOf crm:E55_Type ;
    skos:prefLabel "Military Rank"@en ,
        "Sotilasarvo"@fi .

wacs:hasRank a rdf:Property ;
    rdfs:domain :Promotion ;
    rdfs:range :Rank ;
    skos:prefLabel "Military Rank"@en ,
        "Sotilasarvo"@fi .

wacs:level a rdf:Property ;
   rdfs:domain :Rank ;
   rdfs:range xsd:integer ;
   skos:prefLabel "Level"@en ,
        "Taso"@fi .

org:rankSeniorTo skos:prefLabel "ylempi kuin"@fi .
org:equalTo skos:prefLabel "vastaa"@fi .


## UNIT CATEGORIES

:UnitCategory a rdfs:Class ;
    rdfs:subClassOf crm:E55_Type ;
    skos:prefLabel "Military Unit category"@en,
        "Sotilasosastotyyppi"@fi .

wacs:hasUnitCategory a rdf:Property ;
    rdfs:domain :Group ;
    rdfs:range :UnitCategory ;
    skos:prefLabel "Military Unit category"@en ,
        "sotilasosastotyyppi"@fi .

## WAR DIARIES

:WarDiary a rdfs:Class ;
    rdfs:subClassOf crm:E31_Document ;
    skos:prefLabel "War Diary"@en ,
        "Sotapäiväkirja"@fi .


## MEDALS

:Medal a rdfs:Class ;
    rdfs:subClassOf crm:E24_Physical_Man-Made_Thing ;
    skos:prefLabel "Kunniamitali"@fi .

## CONFLICTS

:Conflict a rdfs:Class ;
    rdfs:subClassOf crm:E4_Period ;
