## ARTICLES

:Article a rdfs:Class ;
    skos:prefLabel "Artikkeli"@fi ;
    rdfs:subClassOf crm:E31_Document .

wars:arms_of_service a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Aselaji"@fi ;
    rdfs:range skos:Concept .

wars:author a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Artikkelin kirjoittaja"@fi ;
    rdfs:range skos:Concept .

wars:event a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Tapahtuma"@fi ;
    rdfs:range skos:Concept .

wars:issue a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Lehden numero"@fi ;
    rdfs:range skos:Concept .

wars:page a owl:DatatypeProperty ;
    rdfs:domain :Article ;
    rdfs:label "Lehden sivu"@fi ;
    rdfs:range xsd:integer .

wars:place a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Paikka"@fi ;
    rdfs:range skos:Concept .

wars:unit a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Joukko-osasto"@fi ;
    rdfs:range skos:Concept .

wars:volume a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Lehden vuosi"@fi ;
    rdfs:range skos:Concept .

wars:mentionsPlace a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Automaattisesti NER-tunnistettu paikka"@fi ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf wars:place, crm:P67_refers_to .

wars:mentionsPerson a owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:label "Automaattisesti NER-tunnistettu henkilö"@fi ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf  crm:P67_refers_to .

